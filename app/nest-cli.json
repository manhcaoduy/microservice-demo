{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/app/tsconfig.app.json",
    "builder": {
      "type": "webpack"
    }
  },
  "monorepo": true,
  "root": "apps/app",
  "projects": {
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "bff": {
      "type": "application",
      "root": "apps/bff",
      "entryFile": "main",
      "sourceRoot": "apps/bff/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bff/tsconfig.app.json"
      }
    },
    "postgres": {
      "type": "library",
      "root": "libs/postgres",
      "entryFile": "index",
      "sourceRoot": "libs/postgres/src",
      "compilerOptions": {
        "tsConfigPath": "libs/postgres/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "caching": {
      "type": "library",
      "root": "libs/caching",
      "entryFile": "index",
      "sourceRoot": "libs/caching/src",
      "compilerOptions": {
        "tsConfigPath": "libs/caching/tsconfig.lib.json"
      }
    },
    "socketer": {
      "type": "application",
      "root": "apps/socketer",
      "entryFile": "main",
      "sourceRoot": "apps/socketer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/socketer/tsconfig.app.json"
      }
    },
    "socket": {
      "type": "library",
      "root": "libs/socket",
      "entryFile": "index",
      "sourceRoot": "libs/socket/src",
      "compilerOptions": {
        "tsConfigPath": "libs/socket/tsconfig.lib.json"
      }
    }
  }
}
